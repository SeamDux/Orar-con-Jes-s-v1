    "web": {
      "favicon": "./assets/favicon-32x32.png"
    },
    "scheme": "orarconjesus",
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "ndkVersion": "28.0.12674087",
            "abiFilters": [
              "arm64-v8a"
            ],
            "packagingOptions": {
              "jniLibs": {
                "useLegacyPackaging": false
              }
            }
          }
        }
      ]
